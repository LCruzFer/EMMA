	keywords = {Consumer Economics: Empirical Analysis, Household Saving, Environmental Taxes and Subsidies, Personal Finance, Micro-Based Behavioral Economics: Role and Effects of Psychological, Emotional, Social, and Cognitive Factors on Decision Making, Macroeconomics: Consumption, Redistributive Effects, Saving, Wealth, Taxation and Subsidies: Externalities},
	pages = {153--183},
	file = {Full Text:/Users/llccf/Zotero/storage/AX4ZQDSQ/Parker - 2017 - Why Don't Households Smooth Consumption Evidence .pdf:application/pdf},
}
