	file = {Submitted Version:/Users/llccf/Zotero/storage/V8GQC8R2/Parker and Souleles - 2019 - Reported Effects versus Revealed-Preference Estima.pdf:application/pdf;Snapshot:/Users/llccf/Zotero/storage/ACPHHNQ9/articles.html:text/html},
