	abstract = {A set of newly-added questions in the 2011–2014 Bank of England/NMG Consulting Survey reveals that British households tend to change their consumption by significantly more in reaction to temporary and unanticipated falls in income than to rises of the same size. Household balance sheet characteristics such as high debt-to-income ratios and small liquidity buffers, concerns about credit market access and higher subjective risk of lower future income account for a sizable share of this spending asymmetry. Our findings have important implications for predicting the response of aggregate consumption to expansionary and contractionary macroeconomic policies.},
	language = {en},
	urldate = {2021-09-15},
	journal = {Journal of Monetary Economics},
	author = {Bunn, Philip and Le Roux, Jeanne and Reinold, Kate and Surico, Paolo},
